{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
      "linebreak-style": [
        0,
        "unix"
      ], 
      "no-param-reassign": 0, 
      "no-return-await": 0, 
      "class-methods-use-this": 0, 
      "max-len": [0, 180], 
      "no-console": 2
    }
}
